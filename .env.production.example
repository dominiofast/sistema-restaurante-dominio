# Configurações de Produção - Remove todos os logs
# Similar ao que empresas como AnotaAí usam
# Copie este arquivo para .env.production

# Debug desabilitado
VITE_DEBUG=false
VITE_LOG_LEVEL=error
VITE_SHOW_PERF=false
VITE_SHOW_API=false
VITE_SHOW_STATE=false

# URLs de produção (substitua pelos seus valores reais)
VITE_API_URL=https://sua-api-producao.com
VITE_SUPABASE_URL=sua_supabase_url
VITE_SUPABASE_ANON_KEY=sua_supabase_key
